<%= form_for @circle do |f| %>

  <div class="form">
    <%= f.label :name, 'Circle Name' %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <h3>Invite Existing Users</h3>
  </div>
  <div class="form-group">
    <h3>Invite New Users</h3>
  </div>

  <div class="actions">
    <%= f.submit class: "btn btn-block btn-lg btn-primary" %>
  </div>
<% end %>
